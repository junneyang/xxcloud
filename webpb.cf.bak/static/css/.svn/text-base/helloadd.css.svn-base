/* ************************************************************************************************ */
/* ***************************************页面元素开始*********************************************** */
/* ************************************************************************************************ */
/* 页头********************************************* */
.mala-header {
  height: 60px;
  background-color: #13b5b1;
}
.mala-header-fix {
  position: fixed;
  top: 0;
  z-index: 10;
  width: 100%;
}
/* logo******************* */
.mala-logo {
  display: block;
  width: 256px;
  height: 40px;
  margin-top: 10px;
  background: url(../images/logo.png) no-repeat center center;
}
.mala-header-fix .mala-logo {
  margin-left: 15px;
}
/* 用户信息******************* */
.mala-user-info {
  position: relative;
}
.mala-header-fix .mala-user-info {
  margin-right: 15px;
}
.mala-username {
  display: inline-block;
  padding: 10px 10px;
  color: #ffffff;
  line-height: 40px;
  vertical-align: top;
  cursor: pointer;
}
.mala-username:hover {
  color: #ffffff;
}
/* 用户头像******************* */
.mala-avatar {
  display: inline-block;
  width: 40px;
  height: 40px;
  overflow: hidden;
  border-radius: 50%;
  margin-top: 10px;
  background: url(../images/avatar.png) no-repeat;
  background-size: cover;
}
.mala-avatar img {
  width: 40px;
  height: 40px;
}
/* 主菜单******************* */
.mala-nav-list {
  margin-right: 60px;
}
.mala-nav-item {
  position: relative;
}
.mala-nav-link {
  display: inline-block;
  width: 120px;
  padding: 10px 0;
  font-size: 15px;
  color: #ffffff;
  text-align: center;
  line-height: 40px;
  cursor: pointer;
}
.mala-nav-link:hover,
.mala-nav-link.active {
  background: #0ca09b;
  color: #ffffff;
  text-decoration: none;
}
/* 下拉菜单******************* */
.mala-header-dropdown-list {
  position: absolute;
  top: 60px;
  z-index: 10;
  display: none;
  text-align: center;
  background: #ffffff;
  width: 100%;
  box-shadow: 0 0 4px #999;
}
.mala-header-dropdown-list .mala-arrow {
  position: absolute;
  top: -6px;
  right: 13px;
  border-width: 6px;
  border-top-color: #ffffff;
}
.mala-header-dropdown-item .mala-link {
  display: block;
  height: 40px;
  line-height: 40px;
}
.mala-header-dropdown-item .mala-link:hover {
  text-decoration: none;
  background: #f1f3f5;
}
/* 轮播********************************************* */
.mala-slider {
  position: relative;
  width: 100%;
  height: 440px;
  overflow: hidden;
}
.mala-slide-list {
  height: 100%;
  overflow: hidden;
  *zoom: 1;
  -webkit-backface-visibility: hidden;
  -webkit-transform-style: preserve-3d;
}
.mala-slide-item {
  height: 100%;
  clear: none;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  text-align: center;
  color: #ffffff;
}
.mala-slide-title {
  font-size: 48px;
  margin-top: 80px;
  font-weight: 500;
}
.mala-slide-subtitle {
  font-size: 36px;
  font-weight: 400;
  margin-bottom: 5px;
}
.mala-slide-cont {
  font-size: 15px;
  line-height: 1.8;
  margin-bottom: 20px;
}
/* 左右箭头******************* */
.slider-arrow {
  position: absolute;
  display: block;
  margin-bottom: -40px;
  width: 40px;
  height: 80px;
  line-height: 999;
  overflow: hidden;
  opacity: .6;
}
.slider-arrow-right {
  bottom: 50%;
  right: 40px;
  background: url(../images/slider-arrow.png) no-repeat -40px 0;
}
.slider-arrow-left {
  bottom: 50%;
  left: 40px;
  background: url(../images/slider-arrow.png) no-repeat 0 0;
}
.slider-arrow:hover {
  opacity: 1;
}
/* 圆点展示******************* */
.slider-nav {
  position: absolute;
  bottom: 30px;
}
.slider-nav-item {
  width: 12px;
  height: 12px;
  float: left;
  clear: none;
  display: block;
  margin: 0 5px;
  background: #fff;
  border-radius: 6px;
  opacity: .4;
  -webkit-transition: background .5s, opacity .5s;
  -moz-transition: background .5s, opacity .5s;
  transition: background .5s, opacity .5s;
}
.slider-nav-item-current {
  opacity: 1;
}
/* 通知栏********************************************* */
.mala-notice {
  padding: 10px 0;
  background: #333333;
  line-height: 40px;
  color: #ffffff;
}
/* 主内容********************************************* */
.mala-main {
  height: auto;
  overflow: hidden;
  margin:0px;
  padding:0px;
}
.mala-figure {
  padding: 15px 0;
}
/* 产品特点********************************************* */
.mala-service-item {
  text-align: center;
}
.mala-service-img {
  display: inline-block;
  width: 96px;
  height: 96px;
  margin-bottom: 10px;
  margin-top:0px;
}
.mala-service-img .fa {
  font-size: 96px;
  color: #13b5b1;
}
.mala-service-title {
  font-size: 24px;
  color: #666666;
}
.mala-service-desc {
  font-size: 15px;
