/* ************************************************************************************************ */
/* ***************************************全局样式************************************************** */
/* ************************************************************************************************ */
* {
	margin: 0;
	padding: 0;
	list-style: none;
}
img{
	border:none;
}
a {
	text-decoration: none;
}
p {
	margin: 4px 0;
}
body {
	margin: 0;
	font-family: "Microsoft YaHei", Helvetica, Arial, sans-serif;
	font-size: 13px;
	line-height: 2;
	color: #333;
	background: #fff;
	text-align: left;
	display: inline;
}
input,
select,
textarea,
label,
button {
  vertical-align: middle;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 13px;
  font-family: "Microsoft Yahei", Helvetica, Arial, sans-serif;
}
.fa {
  margin-right: 5px;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
/* ************************************************************************************************ */
/* ***************************************通用小样式************************************************ */
/* ************************************************************************************************ */
/* 小间距********************************************************** */
.mala-mr {
  margin-right: 10px;
}
.mala-ml {
  margin-left: 10px;
}
/* 浮动方式********************************************************** */
.mala-fl {
  float: left;
}
.mala-fr {
  float: right;
}
.mala-clearfix:after {
  content: '';
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
}
/* 上下小箭头********************************************************** */
.mala-arrow {
  display: inline-block;
  width: 0;
  height: 0;
  overflow: hidden;
  border-right: 4px dashed transparent;
  border-left: 4px dashed transparent;
  cursor: pointer;
  vertical-align: middle;
}
.mala-arrow-up {
  border-bottom: 4px solid #fff;
}
.mala-arrow-up:hover {
  border-bottom-color: #fff;
}
.mala-arrow-down {
  border-top: 4px solid #fff;
}
.mala-arrow-down:hover {
  border-top-color: #fff;
}
/* ************************************************************************************************ */
/* ***************************************按钮样式************************************************** */
/* ************************************************************************************************ */
/* 按钮初始化******** */
.mala-btn {
  display: inline-block;
  font-weight: normal;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
  background-image: none;
  border: 1px solid transparent;
  white-space: nowrap;
  padding: 10px 15px;
  font-size: 13px;
  line-height: 18px;
  border-radius: 2px;
  height: auto;
}
.mala-btn-wide {
  padding: 10px 30px;
}
/* 各种尺寸******** */
/* 翻页按钮的尺寸******** */
.mala-btn-smaller {
  padding: 5px 5px;
  font-size: 12px;
  line-height: 13px;
  border-radius: 1px;
}
/* 列表筛选&表单内按钮******** */
.mala-btn-small {
  padding: 10px 15px;
  font-size: 13px;
  line-height: 13px;
  border-radius: 2px;
}
/* 表单提交按钮尺寸******** */
.mala-btn-large {
  padding: 10px 45px;
  font-size: 15px;
  line-height: 23px;
  border-radius: 2px;
}
/* 首页大按钮尺寸******** */
.mala-btn-larger {
  padding: 15px 50px;
  font-size: 20px;
  line-height: 28px;
  border-radius: 4px;
}
.mala-btn:hover {
  color: #333333;
  text-decoration: none;
}
.mala-btn:active,
.mala-btn.active {
  outline: 0;
  background-image: none;
  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125);
  box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125);
}
.mala-btn-block {
  display: block;
}
/* 禁用的按钮******** */
.mala-disabled {
  cursor: not-allowed;
  pointer-events: none;
  opacity: 0.65;
  filter: alpha(opacity=65);
  -webkit-box-shadow: none;
  box-shadow: none;
}
/* 默认的按钮******** */
.mala-btn-default {
  color: #ffffff;
  background-color: #a6b6c6;
  border-color: #a6b6c6;
}
.mala-btn-default:hover,
.mala-btn-default:focus,
.mala-btn-default:active,
.mala-btn-default.active {
  color: #ffffff;
  background-color: #667686;
  border-color: #667686;
}
.mala-btn-default:active,
.mala-btn-default.active {
  background-image: none;
}
.mala-btn-default.disabled,
.mala-btn-default.disabled:hover,
.mala-btn-default.disabled:focus,
.mala-btn-default.disabled:active,
.mala-btn-default.disabled.active {
  background-color: #f7f8f9;
  border-color: #f1f3f5;
}
/* 主要按钮******** */
.mala-btn-primary {
  color: #ffffff;
  background-color: #13b5b1;
  border-color: #13b5b1;
}
.mala-btn-primary:hover,
.mala-btn-primary:focus,
.mala-btn-primary:active,
.mala-btn-primary.active {
  color: #ffffff;
  background-color: #0ca09b;
  border-color: #0ca09b;
}
.mala-btn-primary:active,
.mala-btn-primary.active {
  background-image: none;
}
.mala-btn-primary.disabled,
.mala-btn-primary.disabled:hover,
.mala-btn-primary.disabled:focus,
.mala-btn-primary.disabled:active,
.mala-btn-primary.disabled.active {
  background-color: #0ca09b;
  border-color: #0ca09b;
}
/* 灰色按钮（多用于模拟下拉菜单）******** */
.mala-btn-info {
  color: #666666;
  background-color: #ffffff;
  border-color: #cccccc;
}
.mala-btn-info:hover,
.mala-btn-info:focus,
.mala-btn-info:active,
.mala-btn-info.active {
  color: #666666;
  background-color: #ffffff;
  border-color: #cccccc;
}
.mala-btn-info:active,
.mala-btn-info.active {
  background-image: none;
}
.mala-btn-info.disabled,
.mala-btn-info.disabled:hover,
.mala-btn-info.disabled:focus,
.mala-btn-info.disabled:active,
.mala-btn-info.disabled.active {
  background-color: #ffffff;
  border-color: #cccccc;
}
/* 成功的按钮******** */
.mala-btn-success {
  color: #ffffff;
  background-color: #7dcc93;
  border-color: #7dcc93;
}
.mala-btn-success:hover,
.mala-btn-success:focus,
.mala-btn-success:active,
.mala-btn-success.active {
  color: #ffffff;
  background-color: #63bc7a;
  border-color: #63bc7a;
}
.mala-btn-success:active,
.mala-btn-success.active {
  background-image: none;
}
.mala-btn-success.disabled,
.mala-btn-success.disabled:hover,
.mala-btn-success.disabled:focus,
.mala-btn-success.disabled:active,
.mala-btn-success.disabled.active {
  background-color: #7dcc93;
  border-color: #7dcc93;
}
/* 危险操作******** */
.mala-btn-danger {
  color: #ffffff;
  background-color: #ff6156;
  border-color: #ff6156;
}
.mala-btn-danger:hover,
.mala-btn-danger:focus,
.mala-btn-danger:active,
.mala-btn-danger.active {
  color: #ffffff;
  background-color: #ff3b30;
  border-color: #ff3b30;
}
.mala-btn-danger:active,
.mala-btn-danger.active {
  background-image: none;
}
.mala-btn-danger.disabled,
.mala-btn-danger.disabled:hover,
.mala-btn-danger.disabled:focus,
.mala-btn-danger.disabled:active,
.mala-btn-danger.disabled.active {
  background-color: #ff6156;
  border-color: #ff6156;
}
/* 按钮组******** */
.mala-btn-group {
  display: inline-block;
}
.mala-btn-group .mala-btn {
  border-radius: 0;
}
.mala-btn-group .mala-btn-smaller:first-child {
  border-bottom-left-radius: 1px;
  border-top-left-radius: 1px;
}
.mala-btn-group .mala-btn:first-child,
.mala-btn-group .mala-btn-small:first-child {
  border-bottom-left-radius: 2px;
  border-top-left-radius: 2px;
}
.mala-btn-group .mala-btn-large:first-child {
  border-bottom-left-radius: 2px;
  border-top-left-radius: 2px;
}
.mala-btn-group .mala-btn-larger:first-child {
  border-bottom-left-radius: 4px;
  border-top-left-radius: 4px;
}
.mala-btn-group .mala-btn-smaller:last-child {
  border-bottom-right-radius: 1px;
  border-top-right-radius: 1px;
}
.mala-btn-group .mala-btn:last-child,
.mala-btn-group .mala-btn-small:last-child {
  border-bottom-right-radius: 2px;
  border-top-right-radius: 2px;
}
.mala-btn-group .mala-btn-large:last-child {
  border-bottom-right-radius: 2px;
  border-top-right-radius: 2px;
}
.mala-btn-group .mala-btn-larger:last-child {
  border-bottom-right-radius: 4px;
  border-top-right-radius: 4px;
}
/* 模拟下拉菜单******** */
.mala-fake-select {
  font-size: 13px;
  position: relative;
  display: inline-block;
}
.mala-select-title {
  padding: 10px 20px 10px 10px;
  border: 1px solid #f1f3f5;
  border-radius: 2px;
  line-height: 13px;
  background: #ffffff;
  display: block;
  cursor: default;
}
.mala-fake-select .mala-arrow {
  position: absolute;
  top: 15.5px;
  right: 8px;
  cursor: default;
}
.mala-fake-select .mala-arrow-down {
  border-top: 4px solid #cccccc;
}
.mala-fake-select .mala-arrow-down:hover {
  border-top-color: #cccccc;
}
.mala-fake-select .mala-select-list {
  position: absolute;
  right: 0;
  z-index: 10;
  white-space: nowrap;
  border-left: 1px solid #f1f3f5;
  border-right: 1px solid #f1f3f5;
  background: #ffffff;
  height: 0;
  overflow: hidden;
  box-shadow: 0 0 2px #ccc;
}
.mala-fake-select .mala-select-list .mala-select-item:last-child {
  border-bottom: 1px solid #f1f3f5;
}
.mala-fake-select .mala-select-list-fix {
  border-left: 1px solid #f1f3f5;
  border-right: 1px solid #f1f3f5;
  height: 0;
  overflow: hidden;
}
.mala-fake-select .mala-select-item {
  cursor: default;
  padding: 10px 20px 10px 10px;
  border-bottom: 1px solid #f1f3f5;
  border-radius: 2px;
  line-height: 14px;
}
.mala-fake-select .mala-select-item:hover {
  background: #f7f8f9;
}
.mala-fake-select .mala-select-item.active {
  background: #f1f3f5;
}
/* ************************************************************************************************ */
/* ***************************************提醒框************************************************** */
/* ************************************************************************************************ */
.mala-alert {
  padding: 10px 20px;
  text-align: left;
  line-height: 20px;
  font-size: 13px;
  background: #f1f3f5;
  color: #a6b6c6;
}
.mala-alert-primary {
  background: #e7f7f7;
  color: #13b5b1;
}
.mala-alert-success {
  background: #f2faf4;
  color: #7dcc93;
}
.mala-alert-danger {
  background: #ffefee;
  color: #ff6156;
}
/* ************************************************************************************************ */
/* ***************************************表单元素************************************************** */
/* ************************************************************************************************ */
/* 输入框**************************************** */
.mala-input[type='text'] {
  font-size: 13px;
  color: #666666;
  width: 160px;
  padding: 5px 10px;
  background: #f1f3f5;
  border: 1px solid #f1f3f5;
  border-radius: order-radius-base;
  height: 35px;
}
.mala-input[type='text']:focus {
  outline: none;
}
.mala-input.mala-input-small[type='text'] {
  width: 120px;
}
.mala-input.mala-input-large[type='text'] {
  width: 320px;
}
/* 一般选择框**************************************** */
.mala-select {
  font-size: 13px;
  color: #666666;
  width: 160px;
  padding: 5px 10px;
  background: #f1f3f5;
  border: 1px solid #f1f3f5;
  border-radius: border-radius-base;
  height: 35px;
}
.mala-select:focus {
  outline: none;
}
.mala-select.mala-select-small[type='text'] {
  width: 120px;
}
.mala-select.mala-select-large[type='text'] {
  width: 320px;
}
/* 文本区**************************************** */
.mala-textarea {
  font-size: 13px;
  color: #666666;
  width: 320px;
  padding: 5px 10px;
  background: #f1f3f5;
  border: 1px solid #f1f3f5;
  border-radius: order-radius-base;
  height: auto;
}
.mala-textarea:focus {
  outline: none;
}
.mala-textarea.mala-textarea-small[type='text'] {
  width: 160px;
}
/* 栅格内的控件, 填充栅格宽度**************************************** */
.mala-input.mala-form-control,
.mala-select.mala-form-control,
.mala-textarea.mala-form-control {
  width: 100%;
}
.mala-form-fix {
  padding-top: 5px;
}
/* ************************************************************************************************ */
/* ***************************************表格基本样式********************************************** */
/* ************************************************************************************************ */
.mala-table {
  font-size: 13px;
  text-align: left;
  width: 100%;
}
.mala-table .mala-th {
  font-weight: 500;
  padding: 10px 0;
  background: #f1f3f5;
  line-height: 25px;
}
.mala-table .mala-tr:nth-of-type(even) {
  background: #f7f8f9;
}
.mala-table .mala-td {
  padding: 10px 0;
  color: #999999;
  line-height: 20px;
}
.mala-tr .mala-td:first-child,
tr .mala-th:first-child {
  padding-left: 15px;
}
.mala-tr .mala-td:last-child,
tr .mala-th:last-child {
  padding-right: 15px;
}
/* ************************************************************************************************ */
/* ***************************************表格中的项目********************************************** */
/* ************************************************************************************************ */
/* 标题与时间***********/
.mala-td-title a {
  color: #667686;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.mala-td-title.mala-td-title-icon a {
  display: list-item;
  list-style-position: inside;
}
.mala-td-title a:hover {
  color: #4697ce;
}
/* ************************************************************************************************ */
/* ***************************************列表***************************************************** */
/* ************************************************************************************************ */
/* **********************一般列表************************/
.mala-list {
  text-align: left;
  width: 100%;
}
.mala-list-item {
  padding: 10px 15px;
  line-height: 20px;
}
.mala-list-item:nth-of-type(even) {
  background: #f7f8f9;
}
.mala-list-title a {
  color: #667686;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.mala-list-title a:hover {
  color: #4697ce;
}
.mala-list-time {
  text-align: right;
  color: #999999;
}
/* **********************左右摘要列表************************/
.mala-list-summary-photo {
  float: left;
  width: 60px;
  height: 60px;
  margin-right: 20px;
}
.mala-list-summary-photo img {
  width: 60px;
  height: 60px;
  border-radius: 50%;
}
.mala-list-summary-title {
  font-size: 15px;
  font-weight: 700;
  color: #333333;
  display: block;
  line-height: 1.8;
  margin-top: 5px;
}
.mala-list-summary-desc {
  font-size: 13px;
  font-weight: 700;
  color: #999999;
  display: block;
  line-height: 1.8;
}
/* **********************图片列表************************/
.mala-grid {
  text-align: left;
  width: 100%;
}
.mala-grid-item {
  float: left;
  width: 25%;
  box-sizing: border-box;
  padding: 10px;
  text-align: center;
}
.mala-grid-summary-photo {
  width: 60px;
  height: 60px;
  margin-right: 20px;
}
.mala-grid-summary-photo img {
  border-radius: 1px;
}
.mala-grid-summary-title {
  font-size: 15px;
  font-weight: 700;
  color: #333333;
  display: block;
  line-height: 1.8;
  margin-top: 5px;
}
/* ************************************************************************************************ */
/* ***************************************echarts占位********************************************** */
/* ************************************************************************************************ */
.mala-echarts-normal {
  height: 300px;
  margin: 0 auto;
}
.mala-echarts-large {
  height: 500px;
  margin: 0 auto;
}
/* ************************************************************************************************ */
/* ***************************************翻页模块************************************************** */
/* ************************************************************************************************ */
.mala-pager {
  font-size: 12px;
  text-align: center;
  padding: 30px 0;
}
.mala-pager .mala-paginate-link {
  display: inline-block;
  width: 25px;
  height: 25px;
  line-height: 25px;
  cursor: pointer;
  border-radius: 1px;
}
.mala-pager .mala-paginate-link:hover {
  background: #f7f8f9;
  text-decoration: none;
}
.mala-pager .mala-paginate-link.active {
  background: #13b5b1;
  color: #ffffff;
}
.mala-pager .mala-paginate-link .mala-caret {
  display: inline-block;
  width: 0;
  height: 0;
  overflow: hidden;
  border-top: 6px dashed transparent;
  border-bottom: 6px dashed transparent;
  vertical-align: middle;
  margin-top: -2px;
}
.mala-pager .mala-paginate-link .mala-caret-prev {
  border-right: 6px dashed #a6b6c6;
}
.mala-pager .mala-paginate-link .mala-caret-next {
  border-left: 6px dashed #a6b6c6;
}
/* ************************************************************************************************ */
/* ***************************************选项卡样式********************************************** */
/* ************************************************************************************************ */
.mala-tab-navi-bar {
  padding: 0 20px;
  background: #f7f8f9;
  margin-bottom: 15px;
}
.mala-tab-navi-bar .mala-tab-navi-item {
  font-size: 15px;
  color: #a6b6c6;
  width: 120px;
  padding: 10px 0;
  line-height: 30px;
  text-align: center;
  border-top-right-radius: 2px;
  border-top-left-radius: 2px;
  cursor: pointer;
}
.mala-tab-navi-bar .mala-tab-navi-item:hover {
  color: #13b5b1;
}
.mala-tab-navi-bar .mala-tab-navi-item.active {
  color: #13b5b1;
  background: #ffffff;
}
.mala-tab-navi-bar .mala-tab-navi-item.active:hover {
  corsor: default;
}
/* ************************************************************************************************ */
/* ***************************************栅格化**************************************************** */
/* ************************************************************************************************ */
.mala-container {
  margin-right: auto;
  margin-left: auto;
  padding-left: 10px;
  padding-right: 10px;
  padding-top:0px;
}
.mala-container:after {
  content: '';
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
}
@media (min-width: 768px) {
  .mala-container {
    width: 740px;
  }
}
@media (min-width: 992px) {
  .mala-container {
    width: 960px;
  }
}
@media (min-width: 1200px) {
  .mala-container {
    width: 1180px;
  }
}
.mala-container-fluid {
  margin-right: auto;
  margin-left: auto;
  padding-left: 20px;
  padding-right: 20px;
}
.mala-container-fluid:after {
  content: '';
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
}
.mala-row {
  margin-left: -10px;
  margin-right: -10px;
}
.mala-row:after {
  content: '';
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
}
.mala-col-xs-1,
.mala-col-sm-1,
.mala-col-md-1,
.mala-col-lg-1,
.mala-col-xs-2,
.mala-col-sm-2,
.mala-col-md-2,
.mala-col-lg-2,
.mala-col-xs-3,
.mala-col-sm-3,
.mala-col-md-3,
.mala-col-lg-3,
.mala-col-xs-4,
.mala-col-sm-4,
.mala-col-md-4,
.mala-col-lg-4,
.mala-col-xs-5,
.mala-col-sm-5,
.mala-col-md-5,
.mala-col-lg-5,
.mala-col-xs-6,
.mala-col-sm-6,
.mala-col-md-6,
.mala-col-lg-6,
.mala-col-xs-7,
.mala-col-sm-7,
.mala-col-md-7,
.mala-col-lg-7,
.mala-col-xs-8,
.mala-col-sm-8,
.mala-col-md-8,
.mala-col-lg-8,
.mala-col-xs-9,
.mala-col-sm-9,
.mala-col-md-9,
.mala-col-lg-9,
.mala-col-xs-10,
.mala-col-sm-10,
.mala-col-md-10,
.mala-col-lg-10,
.mala-col-xs-11,
.mala-col-sm-11,
.mala-col-md-11,
.mala-col-lg-11,
.mala-col-xs-12,
.mala-col-sm-12,
.mala-col-md-12,
.mala-col-lg-12 {
  position: relative;
  min-height: 1px;
  padding-left: 10px;
  padding-right: 10px;
  box-sizing: border-box;
}
.mala-col-xs-1,
.mala-col-xs-2,
.mala-col-xs-3,
.mala-col-xs-4,
.mala-col-xs-5,
.mala-col-xs-6,
.mala-col-xs-7,
.mala-col-xs-8,
.mala-col-xs-9,
.mala-col-xs-10,
.mala-col-xs-11,
.mala-col-xs-12 {
  float: left;
}
