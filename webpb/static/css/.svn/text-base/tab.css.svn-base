/*
   ghostrin
*/

body {
    background-color: #FFFFFF;
    color: #383E4B;
    font:14px/24px \5fae\8f6f\96c5\9ed1;
    margin: 0;
    padding: 0;
    overflow-y:visible;
    overflow-x:hidden;
}

html,body{
    margin: 0;
    padding:0;
    height: 100%;
}

a {
    color: #383E4B;
}

ul,ol{padding: 0; margin: 0}
li{list-style-type:none;}
img{vertical-align:top;border:none;}

h1 {font-size: 22px; line-height: 32px;}
h2 {font-size: 18px; line-height: 28px;}
h3 {font-size: 16px; line-height: 26px;}
h4 {font-size: 14px; line-height: 24px;}
h5 {font-size: 13px; line-height: 24px;}
h6 {font-size: 12px; line-height: 22px;}
h1,h2,h3{font-family: \5fae\8f6f\96c5\9ed1;}

blockquote p{font-size: 14px}

.clearfix:after{content:".";display:block;height:0;clear:both;visibility:hidden;}
.clearfix{display:inline-block;}/* Hides from IE-mac \*/
* html .clearfix{height:1%;}
.clearfix{display:block;}/* End hide from IE-mac */

/* span blank */
.blankW {background:#fff;}
.blank3 {clear:both; height:3px; overflow:hidden; display:block; font-size:1px;}
.blank5 {clear:both; height:5px; overflow:hidden; display:block; font-size:1px;}
.blank10 {clear:both; height:10px; overflow:hidden; display:block; font-size:1px;}
.blank12 {clear:both; height:12px; overflow:hidden; display:block;}
.blank15 {clear:both; height:15px; overflow:hidden; display:block;}
.blank20 {clear:both; height:20px; overflow:hidden; display:block;}

.dotline{ height: 1px; line-height: 0; font-size: 0; border-bottom: #e8e8e8 1px dashed; overflow: hidden;}

#content {
    min-height: 100%;
    width: 100%;
    position: relative;
}
*html .container {
    height: 100%;
}
.container {
    width: 1000px;
    margin: 0 auto;
    min-height:100%;
    height: auto !important;
    height: 100%; /*IE6涓嶈瘑鍒玬in-height*/
    position: relative;
}

/* header content */
#header{ width: 100%; height: 90px; background-color: #00a2ca; position: relative}
.header-top{height:30px; line-height: 30px; margin: 0 auto; background-color: #008fb5; color: #fff; }
.header-top .app-title{ background: url("../images/app-titele-bg.jpg") left center no-repeat; float: left; margin-left: 50px; padding-left: 30px}
.header-top .header-top-acc{position: absolute; right: 10px; top: 0;}
.header-top .header-top-acc li{ float: left; padding: 0 10px;}
.header-top .header-top-acc li a{ color: #fff}

.logo{ float:left; width: 270px; margin-top: 11px; text-align: center;}
.slogn{ margin:15px 0 0 10px;}
.nav{ float: left; display: inline-block}
.nav li{ float: left; line-height: 60px; padding:0 20px; font-size: 18px; display: inline-block}
.nav li a{ color: #fff;}
.nav li a:hover,.nav li a{text-decoration: none; outline: none}
.nav li:hover,.nav li.actived{ background-color: #008fb5; color: #fff;}

/* aside content */
#aside{width: 270px; float: left; left: 0; position: absolute; top: 90px; z-index: 1002;}

.aside-bg{ background: url("../images/aside-bg.gif") left repeat-y;}
.menu{ font-size: 16px; margin-left: 30px}
.menu { margin-top: 15px;}
.menu ul{position: relative}
.menu > ul ul { display: none;}
.menu > ul > li{margin:2px 0;}
.menu > ul > li a{ line-height: 40px; padding-left:10px; }
.menu > ul > li > a > i{ padding: 0 10px}
.menu > ul > li:hover,.menu > ul > li.active{ background-color: #fff; border-radius: 4px 0 0 4px; text-decoration: none}
.menu > ul > li:hover i,.menu > ul> li.active i{ color:#FFBB05}
.menu .m-title{margin: 15px 0; color: #A699B3;}
.menu .m-title:hover{background: none;}
.menu  ul li a:focus {color: #C9C9C9;}
.menu ul li a:hover {text-decoration: none;}
.menu > ul ul li{line-height:40px; background-color:#fafafa; border-top: #e8eaeb 1px solid;}
.menu > ul li b{ float: right; margin-right: 10px}
/* sub li and more li style */
.menu  ul li li {
    border-bottom: 0 none;
    position: relative;
}
.menu > ul ul li:hover{ background:#fff}

.menu > ul > li > ul:before {
    border-left: 1px solid #A8A8A8;
    bottom: 0;
    content: "";
    display: block;
    left: 36px;
    position: absolute;
    top: 0;
    z-index: 1;
}
.menu > ul ul li:before {
    border-top: 1px solid #A8A8A8;
    content: "";
    display: block;
    left: 36px;
    position: absolute;
    top: 20px;
    width: 8px;
    z-index: 1;
}
.menu > ul > li > ul:last-child,.menu > ul ul li:last-child{ border-radius: 0 0 0 6px;}
.menu  > ul ul ul li:before {
    border-top: 1px solid rgba(0, 0, 0, 0);
    content: "";
    display: block;
    left: 10px;
    position: absolute;
    top: 17px;
    width: 18px;
}

.menu ul ul li > a {
    font-size: 14px;
    font-weight: 400;
    outline: 0 none;
    padding-left: 52px;
}

/* main conten */
#main{margin-left:270px; min-height: 500px; display: block; padding: 15px;}

/* Breadcrumb */
.breadcrumb {
    background: none repeat scroll 0 0 rgba(0, 0, 0, 0);
    display: inline-block;
    margin: 0;
    padding: 11px 34px 11px 0 !important;
    vertical-align: top;
}
.breadcrumb, .breadcrumb a {
    color: #BBBBBB !important;
    text-decoration: none !important;
}
.breadcrumb li:last-child, .breadcrumb > .active {
    color: #E4E4E4;
}

h3.title{
    background: #ffffff;
    background-image: -webkit-linear-gradient(top, #fcfcfc, #e6e6e6);
    background-image: -moz-linear-gradient(top, #fcfcfc, #e6e6e6);
    background-image: -ms-linear-gradient(top, #fcfcfc, #e6e6e6);
    background-image: -o-linear-gradient(top, #fcfcfc, #e6e6e6);
    background-image: linear-gradient(to bottom, #fcfcfc, #e6e6e6);
    /*-webkit-box-shadow: 1px 1px 4px #666666;
    -moz-box-shadow: 1px 1px 4px #666666;
    box-shadow: 1px 1px 4px #666666;*/
    border-top:#f6f6f6 1px solid;
    border-bottom: #d9d9d9 1px solid;
    color: #317EAC;
    font-size: 16px;
    text-decoration: none;
    border-right: none;
    border-left: none;
    line-height: 32px;
    padding-left: 20px;
}
h3.title > i{ margin-right: 10px}

.fail{color: #e85542}
.succeed{color: #00b086}
.update{ color: #1b9fe9}

.query-box{background: #f6f6f6; padding: 20px 0 10px}
.query-box label{ font-weight: normal; padding: 0 10px; margin-right: 40px}
.query-box input{ font-size: 12px;}
.query-box .btn-primary{ margin-left: 45px}


.ribbon{ background-color:#F3F3F3; padding: 5px 20px; margin: 15px;}
.ribbon h4{margin-top:3px; margin-bottom: 0 }
.ribbon h4 a{ color: #00B6E9; padding: 0 10px}
.time-list{}
.time-list li{ float: left; padding: 0 10px; display: inline-block;}

.tab-list{margin: 0 15px;}
.tab-list > h4{ margin-top: 0}

.tab-list2{ margin: 0 15px;}
.tab-list2 > h4{ margin-top: 0; margin-bottom: 15px; border-bottom:#ccc 1px solid}
.tab-list2 > h4 a{ border-radius: 4px 4px 0 0; display: inline-block; background-color: #F2F2F2; padding:3px 15px;}
.tab-list2 > h4 a:hover,.tab-list2 > h4 .active{ background-color: #009DD9; text-decoration: none; color: #fff}

.inner-tab{}
.inner-tab h5{margin-top: 20px}
.inner-tab .tab-ontop{ border-bottom:#E4E4E4 1px solid; line-height: 32px; }
.inner-tab .tab-ontop a{font-size: 16px; padding: 7px 12px}
.inner-tab .tab-ontop .active{ border-radius: 6px 6px 0 0; border:#e4e4e4 1px solid; border-bottom:#fff 1px solid}

.table-area{ margin: 15px;}

/* btn */
.btn{
    border: 1px solid rgba(0, 0, 0, 0.1);
}
.btn-default {
    background-color: #ebebeb;
    color: #3e4347;
    border-color: #dddddd;
}
.btn-default:hover, .btn-default:focus, .btn-default:active, .btn-default.active{
    background: #1aa0e8;
    color: #FFFFFF;
    border-color: #198ad7;
}
.btn-primary {
    background: #1aa0e8;
    color: #FFFFFF;
    border-color: #198ad7;
}
.btn-primary:hover, .btn-primary.disabled, .btn-primary[disabled] {
    background-color: #1b73ac;
}
.btn-primary:active, .btn-primary.active {
    background-color: #1C89BE;
}

.btn-gray {
    background: #ececec;
    color: #3f4349;
    border-color: #e8e8e8;
}
.btn-gray:hover, .btn-gray.disabled, .btn-gray[disabled] {
    background-color: #575757;
    color: #fff;
}
.btn-gray:active, .btn-gray.active {
    background-color: #1C89BE;
    color: #fff;
}

.btn-small {
    background: #E5E5E5;
    color: #000;
    padding:3px 12px;
}
.btn-small:hover, .btn-small.disabled, .btn-small[disabled] {
    background-color: #c4c4c4;
}

.btn-warning {
    background: #FABB3D;
    color: #FFFFFF;
}
.btn-warning:hover, .btn-warning.disabled, .btn-warning[disabled] {
    background-color: #F9AA0B;
}
.btn-warning:active, .btn-warning.active {
    background-color: #F4A406;
}
.btn-danger {
    background: #FF5454;
    color: #FFFFFF;
}
.btn-danger:hover, .btn-danger.disabled, .btn-danger[disabled] {
    background-color: #FF2121;
}
.btn-danger:active, .btn-danger.active {
    background-color: #FF1717;
}
.btn-success {
    background: #78CD51;
    color: #FFFFFF;
}
.btn-success:hover, .btn-success.disabled, .btn-success[disabled] {
    background-color: #5DB734;
}
.btn-success:active, .btn-success.active {
    background-color: #59AF32;
}
.btn-info {
    background: #67C2EF;
    color: #FFFFFF;
}
.btn-info:hover, .btn-info.disabled, .btn-info[disabled] {
    background-color: #39AFEA;
}
.btn-info:active, .btn-info.active {
    background-color: #30ACE9;
}
.btn-active {
    background: #468847;
    color: #FFFFFF;
}
.btn-active:hover, .btn-active.disabled, .btn-active[disabled] {
    background-color: #5DB734;
}
.btn-active:active, .btn-active.active {
    background-color: #59AF32;
