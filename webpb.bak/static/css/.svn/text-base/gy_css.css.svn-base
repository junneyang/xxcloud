	padding-left:18px;
	padding-right:18px;
	margin-left:0px;
	margin-right:10px;
}

.qa-div span.q2 {
	font-size:120%;
	color:#fff;
	border-radius:2px;
	background-color:#006cff;
	padding-top:4px;
	padding-bottom:4px;
	padding-left:18px;
	padding-right:18px;
	margin-left:0px;
	margin-right:10px;
}

.qa-div span.a1 {
	color:#fff;
	border-radius:2px;
	background-color:#4b8df8;
	padding:5px;
}
