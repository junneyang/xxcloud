/*
* 页面元素样式，标题栏
*/
.header {
	height: 60px;
	#background: #1e8d60;
	#background: #2F0000;
	background: #17263B;
	line-height: 60px;
	#border-bottom: 4px solid #187e55;
	border-bottom: 4px solid #17263B;
	width: 100%;
	min-width: 1080px;
}
.header .h_logo {
	float: left;
	padding: 10px 20px 0 20px;
	height: 50px;
	margin-left: 15%;
}
.header .h_nav {
	display: inline;
	float: right;
	height: 30px;
	line-height: 24px;
	margin-top: 20px;
	margin-right: 15%;
	padding-right: 20px;
	text-align: right;
	width: 280px;
	z-index: 10;
	color: white;
}
.header .h_nav a {
	color:#FFFFFF;
}

/*
* 页面元素样式，菜单栏
*/
.nav {
	width:100%;
	height:40px;
	background: #17263B;
	border-top: white 2px solid;
}
.nav ul{
	width:1000px;
	height:38px;
	display:block;
	margin:0 auto;
	margin-left: 15%;
}
.nav li{
	float:left;
	width:125px;
	text-align:center;
	font:18px/38px \5fae\8f6f\96c5\9ed1;
	font-weight:bold;
	color:#fff;
	position:relative;
	padding:0
}
.nav li a{
	color:#fff;
	font-size:16px;
	text-decoration:none;
	outline:none;
	overflow:hidden
}
.nav li span{
	display:inline-block;
	width:2px;
	height:20px;
	background:url(../images/nav-seperate.png) no-repeat;
	position:absolute;
	right:-2px;
	top:8px
}
.nav li a:hover,a.nav li:hover{
	border:none;
	text-decoration:none;
	color:#fff
}
.nav li:hover span,.nav li.active span{
	display:none
}
.nav li:hover,.nav li.active{
	#background:url(../images/nav-active-bg.png) repeat-x  #c61a14;
	background: #39F;
	border-bottom: #39F 5px solid;
}

/*
* 页面元素样式，轮播
*/
.helloslider {
	float: left;
	width: 100%;
}

.mala-slider {
  #position: relative;
  #position: absolute;
  float: left;
  width: 100%;
  height: 440px;
  overflow: hidden;
  #border-top: white 1px solid;
}
.mala-slide-list {
  height: 100%;
  overflow: hidden;
  *zoom: 1;
  -webkit-backface-visibility: hidden;
  -webkit-transform-style: preserve-3d;
}
.mala-slide-item {
  height: 100%;
  clear: none;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  text-align: center;
  color: #ffffff;
}
.mala-slide-title {
  font-size: 48px;
  margin-top: 80px;
  font-weight: 500;
}
.mala-slide-subtitle {
  font-size: 36px;
  font-weight: 400;
  margin-bottom: 5px;
}
.mala-slide-cont {
  font-size: 15px;
  line-height: 1.8;
  margin-bottom: 20px;
}
/* 左右箭头******************* */
.slider-arrow {
  position: absolute;
  display: block;
  margin-bottom: -40px;
  width: 40px;
  height: 80px;
  line-height: 999;
  overflow: hidden;
  opacity: .6;
}
.slider-arrow-right {
  bottom: 50%;
  right: 40px;
  background: url(/static/images/slider-arrow.png) no-repeat -40px 0;
}
.slider-arrow-left {
  bottom: 50%;
  left: 40px;
  background: url(/static/images/slider-arrow.png) no-repeat 0 0;
}
.slider-arrow:hover {
  opacity: 1;
}
/* 圆点展示******************* */
.slider-nav {
  position: absolute;
  bottom: 30px;
}
.slider-nav-item {
  width: 12px;
  height: 12px;
  float: left;
  clear: none;
  display: block;
  margin: 0 5px;
  background: #fff;
  border-radius: 6px;
  opacity: .4;
  -webkit-transition: background .5s, opacity .5s;
  -moz-transition: background .5s, opacity .5s;
  transition: background .5s, opacity .5s;
}
.slider-nav-item-current {
  opacity: 1;
}

.mala-fl {
  float: left;
}
.mala-container {
  margin-right: auto;
  margin-left: auto;
  padding-left: 10px;
  padding-right: 10px;
}
.mala-container:after {
  content: '';
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
}
/* ************************************************************************************************ */
/* ***************************************按钮样式************************************************** */
/* ************************************************************************************************ */
/* 按钮初始化******** */
.mala-btn {
  display: inline-block;
  font-weight: normal;
  text-align: center;
  vertical-align: middle;
  cursor: pointer;
  background-image: none;
  border: 1px solid transparent;
  white-space: nowrap;
  padding: 10px 15px;
  font-size: 13px;
  line-height: 18px;
  border-radius: 2px;
  height: auto;
}
.mala-btn-wide {
  padding: 10px 30px;
}
/* 各种尺寸******** */
/* 翻页按钮的尺寸******** */
.mala-btn-smaller {
  padding: 5px 5px;
  font-size: 12px;
  line-height: 13px;
  border-radius: 1px;
}
/* 列表筛选&表单内按钮******** */
.mala-btn-small {
  padding: 10px 15px;
  font-size: 13px;
  line-height: 13px;
  border-radius: 2px;
}
/* 表单提交按钮尺寸******** */
.mala-btn-large {
  padding: 10px 45px;
  font-size: 15px;
  line-height: 23px;
  border-radius: 2px;
}
/* 首页大按钮尺寸******** */
.mala-btn-larger {
  padding: 15px 50px;
  font-size: 20px;
  line-height: 28px;
  border-radius: 4px;
}
.mala-btn:hover {
  color: #333333;
  text-decoration: none;
}
.mala-btn:active,
.mala-btn.active {
  outline: 0;
  background-image: none;
  -webkit-box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125);
  box-shadow: inset 0 3px 5px rgba(0, 0, 0, 0.125);
}
.mala-btn-block {
  display: block;
}
/* 禁用的按钮******** */
.mala-disabled {
  cursor: not-allowed;
  pointer-events: none;
  opacity: 0.65;
  filter: alpha(opacity=65);
  -webkit-box-shadow: none;
  box-shadow: none;
}
/* 默认的按钮******** */
.mala-btn-default {
  color: #ffffff;
  background-color: #a6b6c6;
  border-color: #a6b6c6;
}
.mala-btn-default:hover,
.mala-btn-default:focus,
.mala-btn-default:active,
.mala-btn-default.active {
  color: #ffffff;
  background-color: #667686;
  border-color: #667686;
}
.mala-btn-default:active,
.mala-btn-default.active {
  background-image: none;
}
.mala-btn-default.disabled,
.mala-btn-default.disabled:hover,
.mala-btn-default.disabled:focus,
.mala-btn-default.disabled:active,
.mala-btn-default.disabled.active {
  background-color: #f7f8f9;
  border-color: #f1f3f5;
}
/* 主要按钮******** */
.mala-btn-primary {
  color: #ffffff;
  background-color: #13b5b1;
  border-color: #13b5b1;
}
.mala-btn-primary:hover,
.mala-btn-primary:focus,
.mala-btn-primary:active,
.mala-btn-primary.active {
  color: #ffffff;
  background-color: #0ca09b;
  border-color: #0ca09b;
}
.mala-btn-primary:active,
.mala-btn-primary.active {
  background-image: none;
}
.mala-btn-primary.disabled,
.mala-btn-primary.disabled:hover,
.mala-btn-primary.disabled:focus,
.mala-btn-primary.disabled:active,
.mala-btn-primary.disabled.active {
  background-color: #0ca09b;
  border-color: #0ca09b;
}
/* 灰色按钮（多用于模拟下拉菜单）******** */
.mala-btn-info {
  color: #666666;
  background-color: #ffffff;
  border-color: #cccccc;
}
.mala-btn-info:hover,
.mala-btn-info:focus,
.mala-btn-info:active,
.mala-btn-info.active {
  color: #666666;
  background-color: #ffffff;
  border-color: #cccccc;
}
.mala-btn-info:active,
.mala-btn-info.active {
  background-image: none;
}
.mala-btn-info.disabled,
.mala-btn-info.disabled:hover,
.mala-btn-info.disabled:focus,
.mala-btn-info.disabled:active,
.mala-btn-info.disabled.active {
  background-color: #ffffff;
  border-color: #cccccc;
}
/* 成功的按钮******** */
.mala-btn-success {
  color: #ffffff;
  background-color: #7dcc93;
  border-color: #7dcc93;
}
.mala-btn-success:hover,
.mala-btn-success:focus,
.mala-btn-success:active,
.mala-btn-success.active {
  color: #ffffff;
  background-color: #63bc7a;
  border-color: #63bc7a;
}
.mala-btn-success:active,
.mala-btn-success.active {
  background-image: none;
}
.mala-btn-success.disabled,
.mala-btn-success.disabled:hover,
.mala-btn-success.disabled:focus,
.mala-btn-success.disabled:active,
.mala-btn-success.disabled.active {
  background-color: #7dcc93;
  border-color: #7dcc93;
}
/* 危险操作******** */
.mala-btn-danger {
  color: #ffffff;
  background-color: #ff6156;
  border-color: #ff6156;
}
.mala-btn-danger:hover,
.mala-btn-danger:focus,
.mala-btn-danger:active,
.mala-btn-danger.active {
  color: #ffffff;
  background-color: #ff3b30;
  border-color: #ff3b30;
}
.mala-btn-danger:active,
.mala-btn-danger.active {
  background-image: none;
}
.mala-btn-danger.disabled,
.mala-btn-danger.disabled:hover,
.mala-btn-danger.disabled:focus,
.mala-btn-danger.disabled:active,
.mala-btn-danger.disabled.active {
  background-color: #ff6156;
  border-color: #ff6156;
}
/* 按钮组******** */
.mala-btn-group {
  display: inline-block;
}
.mala-btn-group .mala-btn {
  border-radius: 0;
}
.mala-btn-group .mala-btn-smaller:first-child {
  border-bottom-left-radius: 1px;
  border-top-left-radius: 1px;
}
.mala-btn-group .mala-btn:first-child,
.mala-btn-group .mala-btn-small:first-child {
  border-bottom-left-radius: 2px;
  border-top-left-radius: 2px;
}
.mala-btn-group .mala-btn-large:first-child {
  border-bottom-left-radius: 2px;
  border-top-left-radius: 2px;
}
.mala-btn-group .mala-btn-larger:first-child {
  border-bottom-left-radius: 4px;
  border-top-left-radius: 4px;
}
.mala-btn-group .mala-btn-smaller:last-child {
  border-bottom-right-radius: 1px;
  border-top-right-radius: 1px;
}
.mala-btn-group .mala-btn:last-child,
.mala-btn-group .mala-btn-small:last-child {
  border-bottom-right-radius: 2px;
  border-top-right-radius: 2px;
}
.mala-btn-group .mala-btn-large:last-child {
  border-bottom-right-radius: 2px;
  border-top-right-radius: 2px;
}
.mala-btn-group .mala-btn-larger:last-child {
  border-bottom-right-radius: 4px;
  border-top-right-radius: 4px;
}



